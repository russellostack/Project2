<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div id="buttons" class="demo-content">
          <h1>Fit-or-Fat</h1>
          <h6>(the name is a subject to change)</h6>
          <div id="rfq" class="row">
            <div class="col-md-12">
              <div id="fitness-quotes" class="demo-content">
                <h4>Fitness Quote</h4>
                <p id="fq" class="random-quote"></p>
                <p id="fqa" class="random-quote-author"></p>
              </div>
            </div>
            <div class="col-md-12">
              <h2 class="header">Registered Users</h2>
              <ul>
                {{#each Users}}
                <p class="big text">{{id}}. {{username}}<br>
                  {{/each}}
              </ul>

            </div>
          </div>
          <button type="button" id="signinbtn" class="btn btn-primary btn-lg">Sign in</button>
          <!-- <p>Or</p> -->
          <button type="button" id="newuserbnt" class="btn btn-primary btn-lg">Create New User</button>
          <div class="main-text"></div>
        </div>
      </div>
    </div>
  </div>
</div>

<body>
    <div id="myDiv">
    </div>
    <div id="myDiv1">
    </div>
    <script>
        var timeFrame = 7;
        var timeScale = (timeFrame / timeFrame);
        var targetWeight = 175;
        var targetWeightArray = [];
        for (j = 0; j < timeFrame; j++) {
            targetWeightArray.push(targetWeight);
        }
        var activityArray = [];
